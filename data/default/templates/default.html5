<!DOCTYPE html>
<html$if(lang)$ lang="$lang$"$endif$>
<head>
  <meta charset="utf-8">
  <meta name="generator" content="pandoc">
  <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=yes">
$for(author-meta)$
  <meta name="author" content="$author-meta$">
$endfor$
$if(date-meta)$
  <meta name="dcterms.date" content="$date-meta$">
$endif$
  <title>$if(title-prefix)$$title-prefix$ - $endif$$pagetitle$</title>
  <style type="text/css">code{white-space: pre;}</style>
  <!--[if lt IE 9]>
    <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
  <![endif]-->
$if(quotes)$
  <style type="text/css">q { quotes: "“" "”" "‘" "’"; }</style>
$endif$
  <style type="text/css">
  
  code > span.CodeNormal {color: #000000}
code > span.CodeAtom {color: #221199}
code > span.CodeAttribute {color: #0000CC}
code > span.CodeBracket {color: #999977}
code > span.CodeBuiltin {color: #3300AA}
code > span.CodeComment {color: #AA5500}
code > span.CodeDef {color: #0000FF}
code > span.CodeEm {color: #000000;font-style: italic;}
code > span.CodeError {color: #FF0000}
code > span.CodeHeader {color: #0000FF;font-weight: bold;}
code > span.CodeHr {color: #999999}
code > span.CodeInvalidchar {color: #FF0000}
code > span.CodeKeyword {color: #770088}
code > span.CodeLink {color: #0000CC;text-decoration: underline;}
code > span.CodeMeta {color: #555555}
code > span.CodeNegative {color: #DD4444}
code > span.CodeNumber {color: #116644}
code > span.CodeOperator {color: #000000}
code > span.CodePositive {color: #229922}
code > span.CodeProperty {color: #000000}
code > span.CodePunctuation {color: #000000}
code > span.CodeQualifier {color: #555555}
code > span.CodeQuote {color: #009900}
code > span.CodeString {color: #AA1111}
code > span.CodeStringB {color: #FF5500}
code > span.CodeStrong {color: #000000;font-weight: bold;}
code > span.CodeTag {color: #117700}
code > span.CodeVariable {color: #000000}
code > span.CodeVariableB {color: #0055AA}
code > span.CodeVariableC {color: #008855}

  </style>
$for(css)$
  <link rel="stylesheet" href="$css$">
$endfor$
$if(math)$
  $math$
$endif$
$for(header-includes)$
  $header-includes$
$endfor$
</head>
<body>
$for(include-before)$
$include-before$
$endfor$
$if(title)$
<header>
<h1 class="title">$title$</h1>
$if(subtitle)$
<h1 class="subtitle">$subtitle$</h1>
$endif$
$for(author)$
<h2 class="author">$author$</h2>
$endfor$
$if(date)$
<h3 class="date">$date$</h3>
$endif$
</header>
$endif$
$if(toc)$
<nav id="$idprefix$TOC">
$toc$
</nav>
$endif$
$body$
$for(include-after)$
$include-after$
$endfor$
</body>
</html>
